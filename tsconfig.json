{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "declaration": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "strict": false
  },
}
